[Common]
TaskMgrCWD=$SYSHOME/logs/taskmgr
Directory=$SYSHOME/logs
RestartInterval=43200
MaxRestarts=43200
SleepInterval=1
SleepBetweenExecs=3

Start=<<EOF
Tomcat
Http2ImsgGwy
zeroBalReminders
ftpGetFiles
cmdServer
udpRateSvr_p11
udpRateSvr_p12
udpRateSvr_p13
udpRateSvr_p14
CCudpRateSvr_p1
MVNOudpRateSvr_p11
MVNOudpRateSvr_p12
spudSvr_p1
spudSvr_p2
spudSvr_p3
spudSvr_p4
spudSvr_p5
spudSvr_p6
spudSvr_p7
spudSvr_p8
spudSvr_p9
spudSvr_p10
spudSvr_p11
spudSvr_p12
autoPromo
web_schedulerCLASS
EOF

Stop=<<EOF
web_schedulerCLASS
autoPromo
spudSvr_p12
spudSvr_p11
spudSvr_p10
spudSvr_p9
spudSvr_p8
spudSvr_p7
spudSvr_p6
spudSvr_p5
spudSvr_p4
spudSvr_p3
spudSvr_p2
spudSvr_p1
MVNOudpRateSvr_p12
MVNOudpRateSvr_p11
CCudpRateSvr_p1
udpRateSvr_p14
udpRateSvr_p13
udpRateSvr_p12
udpRateSvr_p11
zeroBalReminders
cmdServer
Http2ImsgGwy
Tomcat
ftpGetFiles
EOF

[Tomcat]
Run=$SYSHOME/apps/tomcat/bin/restartTomcat.sh
Shutdown=$SYSHOME/apps/tomcat/bin/shutdownTomcat.sh

[Http2ImsgGwy]
Run=$SYSHOME/bin/http2imsg_gwy.sh 18181
Shutdown=/usr/bin/curl http://localhost:18181/xmlreq/admin?cmd=shutdown
UserId=ixc
AlertOnExit=yes

[zeroBalReminders]
Run=$SYSHOME/bin/zeroBalReminders.pl > /tmp/zeroBalReminders.out >&1
Shutdown=echo exit > /tmp/zeroBalReminders.stop

[ftpGetFiles]
Run=$SYSHOME/bin/ftpGetFiles.pl > /tmp/ftpGetFiles.out >&1
Shutdown=echo exit > /tmp/ftpGetFiles.stop

[udpRateSvr_p11]
Run=$SYSHOME/bin/udpRateSvr udpRateSvr_p11
Shutdown=kill $PID
UserId=ixc
AlertOnExit=yes

[udpRateSvr_p12]
Run=$SYSHOME/bin/udpRateSvr udpRateSvr_p12
Shutdown=kill $PID
UserId=ixc
AlertOnExit=yes

[udpRateSvr_p13]
Run=$SYSHOME/bin/udpRateSvr udpRateSvr_p13
Shutdown=kill $PID
UserId=ixc
AlertOnExit=yes

[udpRateSvr_p14]
Run=$SYSHOME/bin/udpRateSvr udpRateSvr_p14
Shutdown=kill $PID
UserId=ixc
AlertOnExit=yes

[MVNOudpRateSvr_p11]
Run=$SYSHOME/bin/udpRateSvr MVNOudpRateSvr_p11
Shutdown=kill $PID
UserId=ixc
AlertOnExit=yes

[MVNOudpRateSvr_p12]
Run=$SYSHOME/bin/udpRateSvr MVNOudpRateSvr_p12
Shutdown=kill $PID
UserId=ixc
AlertOnExit=yes

[CCudpRateSvr_p1]
Run=$SYSHOME/bin/CCudpRateSvr CCudpRateSvr_p1
Shutdown=kill $PID
UserId=ixc
AlertOnExit=yes

[spudSvr_p1]
Run=$SYSHOME/bin/SpudSvr -p Listen_spudSvr_p1
UserId=ixc
AlertOnExit=yes
Shutdown=kill $PID

[spudSvr_p2]
Run=$SYSHOME/bin/SpudSvr -p Listen_spudSvr_p2
UserId=ixc
AlertOnExit=yes
Shutdown=kill $PID

[spudSvr_p3]
Run=$SYSHOME/bin/SpudSvr -p Listen_spudSvr_p3
UserId=ixc
AlertOnExit=yes
Shutdown=kill $PID

[spudSvr_p4]
Run=$SYSHOME/bin/SpudSvr -p Listen_spudSvr_p4
UserId=ixc
AlertOnExit=yes
Shutdown=kill $PID

[spudSvr_p5]
Run=$SYSHOME/bin/SpudSvr -p Listen_spudSvr_p5
UserId=ixc
AlertOnExit=yes
Shutdown=kill $PID

[spudSvr_p6]
Run=$SYSHOME/bin/SpudSvr -p Listen_spudSvr_p6
UserId=ixc
AlertOnExit=yes
Shutdown=kill $PID

[spudSvr_p7]
Run=$SYSHOME/bin/SpudSvr -p Listen_spudSvr_p7
UserId=ixc
AlertOnExit=yes
Shutdown=kill $PID

[spudSvr_p8]
Run=$SYSHOME/bin/SpudSvr -p Listen_spudSvr_p8
UserId=ixc
AlertOnExit=yes
Shutdown=kill $PID

[spudSvr_p9]
Run=$SYSHOME/bin/SpudSvr -p Listen_spudSvr_p9
