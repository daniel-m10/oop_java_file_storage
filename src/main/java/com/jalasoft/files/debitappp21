Shutdown=kill $PID

[spudSvr_p11]
Run=$SYSHOME/bin/SpudSvr -p Listen_spudSvr_p11
UserId=ixc
AlertOnExit=yes
Shutdown=kill $PID

[spudSvr_p12]
Run=$SYSHOME/bin/SpudSvr -p Listen_spudSvr_p12
UserId=ixc
AlertOnExit=yes
Shutdown=kill $PID

[cmdServer]
Run=$SYSHOME/etc/cmdServer
Shutdown=date >$SYSHOME/apps/cmdServer/.$HOSTNAME.exit
Host=debitapp21

[autoPromo]
Directory=/u/debit/bin
Run=$SYSHOME/bin/autoPromo.pl > /tmp/autoPromo.out >&1
Shutdown=echo exit > /tmp/autoPromo.stop
AlertOnExit=yes

[web_schedulerCLASS]
Run=$SYSHOME/bin/web_schedulerCLASS.pl > /u/debit/processOutput/web_schedulerCLASS.out 2>&1
Shutdown=echo exit > /u/debit/taskmgr/web_schedulerCLASS.exit
AlertOnExit=yes